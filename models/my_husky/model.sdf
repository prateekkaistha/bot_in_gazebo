  <model name="my_husky">
    <pose>0 0 0.5 0 0 0</pose>

    <!-- Main Chassis -->
     <link name="base_link">
      <!-- Physics -->
      <collision name="collision">
        <!-- <pose>0 0 -0.3 0 0 0</pose> -->
        <pose>0 0 0.1 0 0 0</pose>
        <geometry>
          <box>
            <size>1.0074 0.5709 0.2675</size>
